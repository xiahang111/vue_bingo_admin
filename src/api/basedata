import request from '@/utils/request-local'

export function getAllOrderMaker(query) {

  return request({
    url: process.env.ADMIN_API + '/basedata/getAllOrderMaker',
    method: 'post',
    data: query
  })
}

export function getAllSaleMan(query) {

  return request({
    url: process.env.ADMIN_API + '/basedata/getAllSaleMan',
    method: 'post',
    data: query
  })
}

